<CarpetMaxX>5.2</CarpetMaxX>
<CarpetMinX>-5.2</CarpetMinX>

<!--CarpetArea Y Dimension according to the width of the total carpet area -->
<CarpetMaxY>3.7</CarpetMaxY>
<CarpetMinY>-3.7</CarpetMinY>

<!--FieldArea X Dimension according to the length till the side end lines -->
<FieldMaxX>4.5</FieldMaxX>
<FieldMinX>-4.5</FieldMinX>

<!--FieldArea Y Dimension according to the width till the side end lines -->
<FieldMaxY>3.0</FieldMaxY>
<FieldMinY>-3.0</FieldMinY>

