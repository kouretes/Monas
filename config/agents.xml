<?xml version="1.0" ?>
<!-- Modules to be loaded -->
<!-- ThreadFrequency in Hz StatsCycle=n every n repetitions the agent will log statistics -->

<agent IsRealTime=1 Priority=1 ThreadFrequency=50 StatsCycle=1000>
	<name>KMC</name>
	<activity>MotionController</activity>
	<activity>RobotController</activity>
	<activity>LedHandler</activity>
</agent>

<agent IsRealTime=1 Priority=1 ThreadFrequency=20 StatsCycle=50>
	<name>KVision</name>
	<activity>Vision</activity>
	<activity>Localization </activity>
	<activity>Behavior</activity>
</agent>


<provider  IsRealTime=1 Priority=1 ThreadFrequency=10 StatsCycle=1000>Sensors</provider>

<agent IsRealTime=1 Priority=1 ThreadFrequency=4 StatsCycle=1000>
	<name>ObstacleAvoidance</name>
	<activity>ObstacleAvoidance</activity>
</agent>

