<?xml version="1.0" ?>
<!-- Modules to be loaded -->

<!-- ThreadFrequency in Hz StatsCycle=n every n repetitions the agent will
	log statistics -->




<agent IsRealTime=1 Priority=1 ThreadFrequency=40 StatsCycle=1000>
	<name>KMC</name>
	<activity>MotionController</activity>
	<activity>RobotController</activity>
	<activity>LedHandler</activity>
</agent>

<agent IsRealTime=1 Priority=1 ThreadFrequency=20 StatsCycle=50>
	<name>KVision</name>
	<activity>Vision</activity>
	<activity>Localization</activity>
	<activity>Behavior</activity>
<!--
	<activity>HeadBehavior</activity>
-->
</agent>

<agent IsRealTime=1 Priority=1 ThreadFrequency=10 StatsCycle=1000>
	<name>KSensors</name>
	<activity>Sensors</activity>
</agent>

