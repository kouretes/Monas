<?xml version="1.0" ?>
<!-- 
	Field dimensions IN METERS
	
	2-D Coordinate system with start Point (0,0) corresponding to 
	the centre cross of the field  
-->

<!--CarpetArea X Dimension according to the length of the total carpet area -->
<CarpetMaxX>3.7</CarpetMaxX>
<CarpetMinX>-3.7</CarpetMinX>

<!--CarpetArea Y Dimension according to the width of the total carpet area -->
<CarpetMaxY>2.7</CarpetMaxY>
<CarpetMinY>-2.7</CarpetMinY>

<!--FieldArea X Dimension according to the length till the side end lines -->
<FieldMaxX>3.0</FieldMaxX>
<FieldMinX>-3.0</FieldMinX>

<!--FieldArea Y Dimension according to the width till the side end lines -->
<FieldMaxY>2.0</FieldMaxY>
<FieldMinY>-2.0</FieldMinY>

<!--LeftPenaltyArea X Dimension according to the length till the side end lines -->
<LeftPenaltyAreaMaxX>-2.4</LeftPenaltyAreaMaxX>
<LeftPenaltyAreaMinX>-3.0</LeftPenaltyAreaMinX>

<!--LeftPenaltyArea Y Dimension according to the width till the side end lines -->
<LeftPenaltyAreaMaxY>1.1</LeftPenaltyAreaMaxY>
<LeftPenaltyAreaMinY>-1.1</LeftPenaltyAreaMinY>

<!--RightPenaltyArea X Dimension according to the length till the side end lines -->
<RightPenaltyAreaMaxX>3.0</RightPenaltyAreaMaxX>
<RightPenaltyAreaMinX>2.4</RightPenaltyAreaMinX>

<!--RightPenaltyArea Y Dimension according to the width till the side end lines -->
<RightPenaltyAreaMaxY>1.1</RightPenaltyAreaMaxY>
<RightPenaltyAreaMinY>-1.1</RightPenaltyAreaMinY>

<!--LeftGoalArea X Dimension according to the length till the side end lines -->
<LeftGoalAreaMaxX>-3.0</LeftGoalAreaMaxX>
<LeftGoalAreaMinX>-3.4</LeftGoalAreaMinX>

<!--LeftGoalArea Y Dimension according to the width till the side end lines -->
<LeftGoalAreaMaxY>0.7</LeftGoalAreaMaxY>
<LeftGoalAreaMinY>-0.7</LeftGoalAreaMinY>

<!--RightGoalArea X Dimension according to the length till the side end lines -->
<RightGoalAreaMaxX>3.4</RightGoalAreaMaxX>
<RightGoalAreaMinX>3.0</RightGoalAreaMinX>

<!--RightGoalArea Y Dimension according to the width till the side end lines -->
<RightGoalAreaMaxY>0.7</RightGoalAreaMaxY>
<RightGoalAreaMinY>-0.7</RightGoalAreaMinY>

<!-- -->
<LeftPenaltyMarkX>-1.2</LeftPenaltyMarkX>
<LeftPenaltyMarkY>0.0</LeftPenaltyMarkY>

<!-- -->
<LeftPenaltyMarkSize>0.1</LeftPenaltyMarkSize>

<!-- -->
<RightPenaltyMarkX>1.2</RightPenaltyMarkX>
<RightPenaltyMarkY>0.0</RightPenaltyMarkY>

<!-- -->
<RightPenaltyMarkSize>0.1</RightPenaltyMarkSize>

<!--Diameter of the VERTICAL goal posts -->
<DiameterVPost>0.1</DiameterVPost>

<!--Diameter of the centre circle -->
<DiameterCCircle>1.2</DiameterCCircle>